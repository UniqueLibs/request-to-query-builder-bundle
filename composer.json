{
  "name": "uniquelibs/request-to-query-builder-bundle",
  "type": "symfony-bundle",
  "description": "A bundle to parse requests to doctrine query builder.",
  "keywords": ["Rest", "API"],
  "homepage": "https://github.com/UniqueLibs",
  "license": "MIT",
  "authors": [
    {
      "name": "Unique Libs Community",
      "homepage": "https://github.com/UniqueLibs/ember-data-serializer-bundle/contributors"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/framework-bundle": "~2.3",
    "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
    "doctrine/orm": "~2.4",
    "doctrine/doctrine-bundle": "^1.6",
    "greenlion/php-sql-parser": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "symfony-cmf/testing": "1.3.*",
    "doctrine/phpcr-odm": "~1.3",
    "doctrine/doctrine-fixtures-bundle": "~2.2",
    "liip/functional-test-bundle": "~1.5"
  },
  "autoload": {
    "psr-4": { "UniqueLibs\\RequestToQueryBuilderBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "UniqueLibs\\RequestToQueryBuilderTestBundle\\": "Tests/" }
  }
}